@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix a2a: <http://purl.org/stuff/a2a/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://purl.org/stuff/a2a/>
a owl:Ontology ;
dcterms:title "Agent2Agent Protocol Ontology"@en ;
dcterms:description """The Agent2Agent (A2A) Protocol Ontology defines concepts and relationships
for describing interactions between autonomous agents built on different frameworks."""@en ;
dcterms:created "2024-04-18"^^xsd:date ;
dcterms:modified "2024-04-18"^^xsd:date ;
dcterms:publisher <https://github.com/google/A2A> ;
dcterms:license <http://opensource.org/licenses/MIT> ;
vann:preferredNamespacePrefix "a2a" ;
vann:preferredNamespaceUri "http://purl.org/stuff/a2a/" ;
owl:versionInfo "1.0.0" .

# Ontology Definition

a2a: a owl:Ontology ;
dcterms:title "Agent2Agent Protocol Ontology"@en ;
dcterms:description "Ontology for describing Agent2Agent Protocol concepts and relationships"@en ;
dcterms:created "2024-04-18"^^xsd:date ;
vann:preferredNamespacePrefix "a2a" ;
vann:preferredNamespaceUri "http://purl.org/stuff/a2a/" ;
owl:versionInfo "1.0.0" .

# Core Classes

a2a:AgentCard a owl:Class ;
rdfs:label "Agent Card"@en ;
rdfs:comment "A description of an agent's capabilities, authentication requirements, and metadata"@en .

a2a:Task a owl:Class ;
rdfs:label "Task"@en ;
rdfs:comment "A stateful entity that allows clients and remote agents to achieve a specific outcome and generate results"@en .

a2a:Message a owl:Class ;
rdfs:label "Message"@en ;
rdfs:comment "Content exchanged between a client and a remote agent, including context, instructions, or status updates"@en .

a2a:Artifact a owl:Class ;
rdfs:label "Artifact"@en ;
rdfs:comment "An immutable result generated by an agent as the outcome of a task"@en .

a2a:Part a owl:Class ;
rdfs:label "Part"@en ;
rdfs:comment "A fully formed piece of content exchanged between a client and a remote agent"@en .

a2a:TextPart a owl:Class ;
rdfs:subClassOf a2a:Part ;
rdfs:label "Text Part"@en ;
rdfs:comment "A part containing textual content"@en .

a2a:FilePart a owl:Class ;
rdfs:subClassOf a2a:Part ;
rdfs:label "File Part"@en ;
rdfs:comment "A part containing file content"@en .

a2a:DataPart a owl:Class ;
rdfs:subClassOf a2a:Part ;
rdfs:label "Data Part"@en ;
rdfs:comment "A part containing structured data"@en .

a2a:FileContent a owl:Class ;
rdfs:label "File Content"@en ;
rdfs:comment "The content of a file, either as base64 encoded bytes or a URI"@en .

a2a:TaskStatus a owl:Class ;
rdfs:label "Task Status"@en ;
rdfs:comment "The current state of a task and accompanying message"@en .

a2a:PushNotificationConfig a owl:Class ;
rdfs:label "Push Notification Configuration"@en ;
rdfs:comment "Configuration for asynchronous notifications outside of a connected session"@en .

a2a:Skill a owl:Class ;
rdfs:label "Skill"@en ;
rdfs:comment "A unit of capability that an agent can perform"@en .

a2a:AgentProvider a owl:Class ;
rdfs:label "Agent Provider"@en ;
rdfs:comment "Information about the provider of an agent"@en .

a2a:AgentCapabilities a owl:Class ;
rdfs:label "Agent Capabilities"@en ;
rdfs:comment "Optional capabilities supported by an agent"@en .

a2a:AgentAuthentication a owl:Class ;
rdfs:label "Agent Authentication"@en ;
rdfs:comment "Authentication requirements for an agent"@en .

a2a:TaskState a owl:Class ;
rdfs:label "Task State"@en ;
rdfs:comment "The state of a task: submitted, working, input-required, completed, canceled, failed, or unknown"@en .

# Properties

a2a:id a owl:DatatypeProperty ;
rdfs:domain [
  owl:unionOf (a2a:Task a2a:Skill)
] ;
rdfs:range xsd:string ;
rdfs:label "id"@en .

a2a:name a owl:DatatypeProperty ;
rdfs:domain [
  owl:unionOf (a2a:AgentCard a2a:Skill a2a:Artifact a2a:FileContent)
] ;
rdfs:range xsd:string ;
rdfs:label "name"@en .

a2a:description a owl:DatatypeProperty ;
rdfs:domain [
  owl:unionOf (a2a:AgentCard a2a:Skill a2a:Artifact)
] ;
rdfs:range xsd:string ;
rdfs:label "description"@en .

a2a:url a owl:DatatypeProperty ;
rdfs:domain [
  owl:unionOf (a2a:AgentCard a2a:AgentProvider a2a:PushNotificationConfig)
] ;
rdfs:range xsd:string ;
rdfs:label "URL"@en .

a2a:version a owl:DatatypeProperty ;
rdfs:domain a2a:AgentCard ;
rdfs:range xsd:string ;
rdfs:label "version"@en .

a2a:organizationName a owl:DatatypeProperty ;
rdfs:domain a2a:AgentProvider ;
rdfs:range xsd:string ;
rdfs:label "organization name"@en .

a2a:documentationUrl a owl:DatatypeProperty ;
rdfs:domain a2a:AgentCard ;
rdfs:range xsd:string ;
rdfs:label "documentation URL"@en .

a2a:text a owl:DatatypeProperty ;
rdfs:domain a2a:TextPart ;
rdfs:range xsd:string ;
rdfs:label "text"@en .

a2a:data a owl:ObjectProperty ;
rdfs:domain a2a:DataPart ;
rdfs:label "data"@en .

a2a:file a owl:ObjectProperty ;
rdfs:domain a2a:FilePart ;
rdfs:range a2a:FileContent ;
rdfs:label "file"@en .

a2a:mimeType a owl:DatatypeProperty ;
rdfs:domain a2a:FileContent ;
rdfs:range xsd:string ;
rdfs:label "MIME type"@en .

a2a:bytes a owl:DatatypeProperty ;
rdfs:domain a2a:FileContent ;
rdfs:range xsd:base64Binary ;
rdfs:label "bytes"@en .

a2a:uri a owl:DatatypeProperty ;
rdfs:domain a2a:FileContent ;
rdfs:range xsd:anyURI ;
rdfs:label "URI"@en .

a2a:state a owl:ObjectProperty ;
rdfs:domain a2a:TaskStatus ;
rdfs:range a2a:TaskState ;
rdfs:label "state"@en .

a2a:timestamp a owl:DatatypeProperty ;
rdfs:domain a2a:TaskStatus ;
rdfs:range xsd:dateTime ;
rdfs:label "timestamp"@en .

a2a:hasStatus a owl:ObjectProperty ;
rdfs:domain a2a:Task ;
rdfs:range a2a:TaskStatus ;
rdfs:label "has status"@en .

a2a:hasMessage a owl:ObjectProperty ;
rdfs:domain [
  owl:unionOf (a2a:Task a2a:TaskStatus)
] ;
rdfs:range a2a:Message ;
rdfs:label "has message"@en .

a2a:hasArtifact a owl:ObjectProperty ;
rdfs:domain a2a:Task ;
rdfs:range a2a:Artifact ;
rdfs:label "has artifact"@en .

a2a:hasPart a owl:ObjectProperty ;
rdfs:domain [
  owl:unionOf (a2a:Message a2a:Artifact)
] ;
rdfs:range a2a:Part ;
rdfs:label "has part"@en .

a2a:sessionId a owl:DatatypeProperty ;
rdfs:domain a2a:Task ;
rdfs:range xsd:string ;
rdfs:label "session ID"@en .

a2a:role a owl:DatatypeProperty ;
rdfs:domain a2a:Message ;
rdfs:range xsd:string ;
rdfs:label "role"@en .

a2a:hasPushNotificationConfig a owl:ObjectProperty ;
rdfs:domain a2a:Task ;
rdfs:range a2a:PushNotificationConfig ;
rdfs:label "has push notification configuration"@en .

a2a:hasSkill a owl:ObjectProperty ;
rdfs:domain a2a:AgentCard ;
rdfs:range a2a:Skill ;
rdfs:label "has skill"@en .

a2a:hasProvider a owl:ObjectProperty ;
rdfs:domain a2a:AgentCard ;
rdfs:range a2a:AgentProvider ;
rdfs:label "has provider"@en .

a2a:hasCapabilities a owl:ObjectProperty ;
rdfs:domain a2a:AgentCard ;
rdfs:range a2a:AgentCapabilities ;
rdfs:label "has capabilities"@en .

a2a:hasAuthentication a owl:ObjectProperty ;
rdfs:domain [
  owl:unionOf (a2a:AgentCard a2a:PushNotificationConfig)
] ;
rdfs:range a2a:AgentAuthentication ;
rdfs:label "has authentication"@en .

a2a:streaming a owl:DatatypeProperty ;
rdfs:domain a2a:AgentCapabilities ;
rdfs:range xsd:boolean ;
rdfs:label "streaming"@en .

a2a:pushNotifications a owl:DatatypeProperty ;
rdfs:domain a2a:AgentCapabilities ;
rdfs:range xsd:boolean ;
rdfs:label "push notifications"@en .

a2a:stateTransitionHistory a owl:DatatypeProperty ;
rdfs:domain a2a:AgentCapabilities ;
rdfs:range xsd:boolean ;
rdfs:label "state transition history"@en .

a2a:schemes a owl:DatatypeProperty ;
rdfs:domain a2a:AgentAuthentication ;
rdfs:range rdf:List ;
rdfs:label "schemes"@en .

a2a:credentials a owl:DatatypeProperty ;
rdfs:domain a2a:AgentAuthentication ;
rdfs:range xsd:string ;
rdfs:label "credentials"@en .

a2a:defaultInputModes a owl:DatatypeProperty ;
rdfs:domain a2a:AgentCard ;
rdfs:range rdf:List ;
rdfs:label "default input modes"@en .

a2a:defaultOutputModes a owl:DatatypeProperty ;
rdfs:domain a2a:AgentCard ;
rdfs:range rdf:List ;
rdfs:label "default output modes"@en .

a2a:inputModes a owl:DatatypeProperty ;
rdfs:domain a2a:Skill ;
rdfs:range rdf:List ;
rdfs:label "input modes"@en .

a2a:outputModes a owl:DatatypeProperty ;
rdfs:domain a2a:Skill ;
rdfs:range rdf:List ;
rdfs:label "output modes"@en .

a2a:tags a owl:DatatypeProperty ;
rdfs:domain a2a:Skill ;
rdfs:range rdf:List ;
rdfs:label "tags"@en .

a2a:examples a owl:DatatypeProperty ;
rdfs:domain a2a:Skill ;
rdfs:range rdf:List ;
rdfs:label "examples"@en .

a2a:token a owl:DatatypeProperty ;
rdfs:domain a2a:PushNotificationConfig ;
rdfs:range xsd:string ;
rdfs:label "token"@en .

a2a:index a owl:DatatypeProperty ;
rdfs:domain a2a:Artifact ;
rdfs:range xsd:integer ;
rdfs:label "index"@en .

a2a:append a owl:DatatypeProperty ;
rdfs:domain a2a:Artifact ;
rdfs:range xsd:boolean ;
rdfs:label "append"@en .

a2a:lastChunk a owl:DatatypeProperty ;
rdfs:domain a2a:Artifact ;
rdfs:range xsd:boolean ;
rdfs:label "last chunk"@en .

a2a:metadata a owl:DatatypeProperty ;
rdfs:label "metadata"@en .

# Task States as Instances

a2a:SubmittedState a a2a:TaskState ;
rdfs:label "Submitted"@en ;
rdfs:comment "Task has been submitted but processing has not yet begun"@en .

a2a:WorkingState a a2a:TaskState ;
rdfs:label "Working"@en ;
rdfs:comment "Task is currently being processed by the agent"@en .

a2a:InputRequiredState a a2a:TaskState ;
rdfs:label "Input Required"@en ;
rdfs:comment "Agent requires additional input from the client to continue"@en .

a2a:CompletedState a a2a:TaskState ;
rdfs:label "Completed"@en ;
rdfs:comment "Task has been successfully completed"@en .

a2a:CanceledState a a2a:TaskState ;
rdfs:label "Canceled"@en ;
rdfs:comment "Task has been canceled by the client or agent"@en .

a2a:FailedState a a2a:TaskState ;
rdfs:label "Failed"@en ;
rdfs:comment "Task has failed to complete successfully"@en .

a2a:UnknownState a a2a:TaskState ;
rdfs:label "Unknown"@en ;
rdfs:comment "Task state is unknown or undefined"@en .

# Role Values

a2a:UserRole a owl:NamedIndividual ;
rdfs:label "User"@en .

a2a:AgentRole a owl:NamedIndividual ;
rdfs:label "Agent"@en .